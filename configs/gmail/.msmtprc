# Set default values for all following accounts.
defaults
auth            oauthbearer
tls             on
tls_starttls    on
# Check your OS; if on macOS, this path might differ (e.g., /usr/local/etc/openssl/cert.pem)
tls_trust_file  /etc/ssl/certs/ca-certificates.crt
logfile         ~/.msmtp.log

# Gmail Account
account         gmail
host            smtp.gmail.com
port            587
from            YOUREMAILHERE
user            YOUREMAILHERE
# This calls your custom DIY token script
passwordeval    python3 ~/dewlinux/scripts/get_temp_acc_token.py

# Set a default account
account default : gmail

